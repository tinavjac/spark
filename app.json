{
  "expo": {
    "newArchEnabled": true,
    "name": "Spark",
    "slug": "spark",
    "scheme": "spark",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {},
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
